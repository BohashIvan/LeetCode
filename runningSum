// 1. 
var runningSum = function(nums) {
   nums.reduce(function (previousValue, currentValue, i) {
		return nums[i] = previousValue + currentValue;
	}, 0);
	return nums;    
};

// 2.
var runningSum = function (nums) {
	var sumArr = [],
      counter = 0,
      i,
      size = nums.length;
      
	for (i = 0; i < size; i += 1) {
		counter += nums[i];
		sumArr.push(counter);
	}
	return sumArr;
};

// input: [1,2,3,4] 
// output: [1,3,6,10]
